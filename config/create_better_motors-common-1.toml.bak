#Make sure config changes are duplicated on both Clients and the Server when running a dedicated Server,
# as the config isnt synced between Clients and Server.
#Messages
[Messages]
	#If Update Messages should be enabled or not.
	Messages_enabled = true

#Accumulator
[accumulator]
	#Accumulator max input in FE/t (Energy transfer).
	# Default: 800000
	# Range: > 0
	accumulator_max_input = 800000
	#Accumulator max output in FE/t (Energy transfer).
	# Default: 800000
	# Range: > 0
	accumulator_max_output = 800000

#Wires
[wires]
	#Heavy Connector max input in FE/t (Energy transfer).
	# Default: 90000
	# Range: > 0
	Heavy_connector_max_input = 90000
	#Heavy Connector max output in FE/t (Energy transfer).
	# Default: 90000
	# Range: > 0
	Heavy_connector_max_output = 90000
	#Heavy Connector max wire length in blocks.
	# Default: 48
	# Range: 0 ~ 256
	Heavy_connector_wire_length = 48

#Starter Motor
[starter_motor]
	#Electric Motor min/max RPM.
	# Default: 256
	# Range: > 1
	motor_rpm_range = 256
	#Electric Motor minimum required energy consumption in FE/t.
	# Default: 8
	# Range: > 0
	motor_min_consumption = 8
	#Electric Motor max input in FE (Energy transfer not consumption).
	# Default: 80000
	# Range: > 0
	motor_max_input = 80000
	#Electric Motor internal capacity in FE.
	# Default: 100000
	# Range: > 0
	motor_capacity = 100000
	#Forge Energy conversion rate (in FE/t at 256 RPM, value is the FE/t generated and consumed is at 256rpm).
	# Default: 960
	# Range: > 0
	fe_at_max_rpm = 960
	#Max stress for the Alternator and Electric Motor (in SU at 256 RPM).
	# Default: 35768
	# Range: > 0
	max_stress = 35768
	#If audio should be enabled or not.
	audio_enabled = true

